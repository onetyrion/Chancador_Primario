    "node": "10.16.0",
    "npm": "6.14.5"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "4.9.1",
    "classnames": "2.2.6",
    "express": "^4.17.1",
    "history": "4.10.1",
